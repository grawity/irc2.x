/alias a /away
/alias c /channel
/alias ch /channel
/alias chop /mo +o
/alias e /exit
/alias h /help
/alias i /invite $* $+
/alias k /kick $+
/alias l /list
/alias log /set log
/alias m /msg
/alias me /notice $+ $*\\n/echo $N $*
/alias mo /mode $+
/alias n /names
/alias s /signoff
/alias t /topic
/alias unchop /mo -o
/alias w /whois
/on ^public_msg * /echo ($0) $2-
/on ^public_notice * /echo $0 $2-
/on ^wall * /echo ### Broadcast message from $0: "$1-" 
/on ^notify_signon * /echo *** Signon by $0 onto $1 detected
/on ^notify_signoff * /echo *** Signoff by $0 from $1 detected
/comment lynx90
